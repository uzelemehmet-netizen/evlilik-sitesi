{
  "name": "web-sitem-new",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "ensure:env": "node scripts/ensure-env-local.mjs",
    "admin:check": "node scripts/admin-user.mjs --email uzelemehmet@gmail.com --check",
    "admin:setpw": "node scripts/admin-user.mjs --email uzelemehmet@gmail.com",
    "admin:grant": "node scripts/admin-user.mjs --email uzelemehmet@gmail.com --set-admin-claim",
    "admin:revoke": "node scripts/admin-user.mjs --email uzelemehmet@gmail.com --clear-admin-claim",
    "seed:matchmaking": "node scripts/seed-matchmaking.mjs",
    "seed:matchmaking:cleanup": "node scripts/seed-matchmaking.mjs --cleanup",
    "seed:promote-matches": "node scripts/promote-seed-matches.mjs",
    "matchmaking:cron": "node scripts/run-matchmaking-cron.mjs",
    "matchmaking:debug-user": "node scripts/matchmaking-debug-user.mjs",
    "dev:web": "npm run ensure:env && vite",
    "dev:web:direct": "vite",
    "dev:api": "node --watch scripts/dev-api.mjs",
    "dev": "node scripts/dev.mjs",
    "playwright:install": "playwright install chromium",
    "playwright:install:deps": "playwright install --with-deps chromium",
    "playwright:install-deps": "playwright install-deps chromium",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "generate:brochure-pdfs": "node scripts/generate-brochure-pdfs.mjs",
    "generate:brochure-pdfs:strict": "node scripts/generate-brochure-pdfs.mjs --strict",
    "build": "vite build",
    "build:full": "node scripts/generate-brochure-pdfs.mjs && vite build",
    "preview": "vite preview",
    "lint": "node -e \"console.log('lint bilinçli olarak kapalı (performans için). İstersen: npm run lint:full')\"",
    "lint:full": "eslint --cache --cache-location .eslintcache \"src/**/*.{js,jsx}\""
  },
  "dependencies": {
    "@cloudinary/react": "^1.14.3",
    "@cloudinary/url-gen": "^1.22.0",
    "@emailjs/browser": "^4.4.1",
    "firebase": "^12.8.0",
    "firebase-admin": "^13.0.2",
    "i18next": "^25.7.3",
    "lucide-react": "^0.344.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^16.5.0",
    "react-router-dom": "^6.20.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^9.1.2",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.7",
    "globals": "^15.14.0",
    "playwright": "^1.51.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "vercel": "^34.3.0",
    "vite": "^5.0.8",
    "wait-on": "^7.2.0"
  }
}
